<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>app</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^5.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="jpush-phonegap-plugin" spec="file:jpush-phonegap-plugin">
        <variable name="APP_KEY" value="72a124297a8ee1c90346f871" />
    </plugin>
    <plugin name="Device" spec="^0.3.8" />
    <plugin name="cordova-plugin-device-motion" spec="^1.2.5" />
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="device" spec="^0.3.8" />
</widget>
